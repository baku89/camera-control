{
	"name": "tethr",
	"author": "Baku Hashimoto <mail@baku89.com> (https://baku89.com)",
	"version": "0.0.0",
	"description": "Tethers USB-connected cameras, webcam, and smartphone camera from browser",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"dev": "parcel demo/index.html --no-cache --public-url . --dist-dir public",
		"lint": "eslint --fix --ext .ts --ignore-path .gitignore .",
		"format": "prettier --write src demo",
		"fix": "npm run format && npm run lint",
		"build": "tsc --project tsconfig.prod.json --outDir dist",
		"deploy:demo": "gh-pages -d public",
		"prepare": "npm run build"
	},
	"files": [
		"build"
	],
	"homepage": "https://baku89.github.io/tethr/",
	"license": "MIT",
	"dependencies": {
		"bim": "^1.3.3",
		"date-format-parse": "^0.2.7",
		"eventemitter3": "^4.0.7",
		"lodash": "^4.17.21",
		"promise-queue": "^2.2.5",
		"sleep-promise": "^9.1.0"
	},
	"devDependencies": {
		"@parcel/transformer-stylus": "2.0.0-rc.0",
		"@parcel/transformer-typescript-tsc": "2.0.0-rc.0",
		"@parcel/transformer-vue": "2.0.0-rc.0",
		"@parcel/validator-typescript": "^2.0.0-rc.0",
		"@types/file-saver": "^2.0.3",
		"@types/lodash": "^4.14.172",
		"@types/promise-queue": "^2.2.0",
		"@types/w3c-image-capture": "^1.0.5",
		"@types/w3c-web-usb": "^1.0.5",
		"@typescript-eslint/eslint-plugin": "^4.28.2",
		"@typescript-eslint/parser": "^4.28.2",
		"eslint": "^7.30.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-unused-imports": "^1.1.2",
		"file-saver": "^2.0.5",
		"parcel": "^2.0.0-rc.0",
		"prettier": "^2.3.2",
		"typescript": "^4.4.2",
		"vue": "^3.2.9"
	},
	"browserslist": [
		"since 2020-12"
	]
}
